# 注册自定义系统初始化器
org.springframework.context.ApplicationContextInitializer=top.jionjion.web.initializer.WebApplicationInitializerFirst

# 注册自定义系统监听器
org.springframework.context.ApplicationListener=top.jionjion.web.listener.WebApplicationListenerFirst,top.jionjion.web.listener.WebApplicationListenerFourth

# 自定义异常处理报告
org.springframework.boot.SpringBootExceptionReporter=top.jionjion.web.except.WebApplicationExceptionReporter